{
  "name": "z2s",
  "description": "ZQL to SQL compiler. Used for testing the ZQL implementation and reads inside of custom mutators",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src/",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@databases/escape-identifier": "^1.0.3",
    "@databases/sql": "^3.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@rocicorp/eslint-config": "^0.7.0",
    "@rocicorp/prettier-config": "^0.3.0",
    "@types/pg": "^8.11.11",
    "@vitest/runner": "3.2.4",
    "pg": "^8.16.3",
    "vitest": "3.2.4"
  },
  "eslintConfig": {
    "extends": "../../eslint-config.json"
  },
  "prettier": "@rocicorp/prettier-config"
}
